#include <stdio.h>
#include <string.h>

int main(){

    char SLA[100000] = "";
    scanf("%s", SLA);


    int K = strlen(SLA);
    int i = 0;
    int a = 0;
    int c = 0;
    int g = 0;
    int t = 0;

    for (i;i < K; i++){
        if ( SLA[i] == 'A'){
            a = a + 1;
        }
        else if (SLA[i] == 'C'){
            c = c + 1;
        }
        else if (SLA[i] == 'G'){
            g = g + 1;
        }
        else if (SLA[i] == 'T'){
            t = t + 1;
        }
    }


    if ( a >= c){}
    else {
        a = c;
    }

    if ( a >= g){}
    else {
        a = g;
    }
    
    if (a >= t){}
    else{
        a = t;
    }

    printf ("%d", a);
    return 0;
}

